{
  "name": "node-base",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn nodemon ./src/server.js --ignore __tests__",
    "d_up": "docker-compose up",
    "d_console": "docker-compose exec app sh",
    "drop_test_db": "NODE_ENV=test sequelize db:drop",
    "create_test_db": "NODE_ENV=test sequelize db:create",
    "migrate_test_db": "NODE_ENV=test sequelize db:migrate",
    "setup_test_db": "yarn drop_test_db && yarn create_test_db && yarn migrate_test_db",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.1",
    "sequelize": "^5.21.4"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
